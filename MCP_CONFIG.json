{
  "cline.mcp.servers": {
    "iris-execute-mcp": {
      "autoApprove": [
        "execute_command",
        "execute_classmethod",
        "get_global",
        "set_global",
        "get_system_info"
      ],
      "disabled": false,
      "timeout": 60,
      "command": "C:/iris-execute-mcp/venv/Scripts/python.exe",
      "args": ["C:/iris-execute-mcp/iris_execute_fastmcp.py"],
      "env": {
        "IRIS_HOSTNAME": "localhost",
        "IRIS_PORT": "1972",
        "IRIS_NAMESPACE": "HSCUSTOM",
        "IRIS_USERNAME": "_SYSTEM",
        "IRIS_PASSWORD": "_SYSTEM"
      },
      "transportType": "stdio"
    }
  }
}
